// Amusez vous à bidouiller les couleurs
// Forkez le projet et partez de là pour créer votre propre thème carrément

{
    "name": "La Capsule",
    "type": "dark",
    "colors": {
        "editor.background": "#222229",
        "editor.foreground": "#fff0f595",
        "editor.selectionBackground": "#7168eb50",
        "editor.lineHighlightBackground": "#FFF0F507",
        "editorCursor.foreground": "#ffcd03",
        "editorWhitespace.foreground": "#3B3B3B",
        "editorLineNumber.foreground": "#3b4451",
        "editorLineNumber.activeForeground": "#ffcd03",
        "editorGutter.background": "#222229",
        "editorBracketMatch.background": "#264f78",
        "editorBracketMatch.border": "#ef8a39",
        "editorGroup.border": "#1f2933",
        "editorGroupHeader.tabsBackground": "#1d1e24",
        "editorGroupHeader.tabsBorder": "#1f2933",
        "tab.activeBackground": "#ef8a39",
        "tab.inactiveBackground": "#222229",
        "tab.activeForeground": "#222229",
        "tab.inactiveForeground": "#6e7681",
        "tab.border": "#1f2933",
        "activityBar.background": "#1d1e24",
        "activityBar.foreground": "#fff0f595",
        "activityBarBadge.background": "#ef8a39",
        "activityBarBadge.foreground": "#0d1117",
        "sideBar.background": "#1d1e24",
        "sideBar.foreground": "#fff0f595",
        "sideBarSectionHeader.background": "#1d1e24",
        "sideBarSectionHeader.foreground": "#fff0f595",
        "statusBar.background": "#1d1e24",
        "statusBar.foreground": "#fff0f595",
        "statusBar.debuggingBackground": "#800000",
        "statusBar.debuggingForeground": "#fff0f595",
        "editorError.foreground": "#ff5f56",
        "editorWarning.foreground": "#ffbd2e",
        "editorInfo.foreground": "#ef8a39",
        "editorHint.foreground": "#c586c0",
        "editorLightBulb.foreground": "#ffcc00",
        "editorLightBulbAutoFix.foreground": "#ef8a39",
        "editorHoverWidget.background": "#1d1e24",
        "editorHoverWidget.border": "#30363d",
        "editorSuggestWidget.background": "#1d1e24",
        "editorSuggestWidget.border": "#30363d",
        "editorSuggestWidget.foreground": "#fff0f595",
        "editorSuggestWidget.selectedBackground": "#264f78",
        "editorSuggestWidget.highlightForeground": "#ef8a39",
        "editorWidget.background": "#1d1e24",
        "editorWidget.border": "#30363d",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#ff5f56",
        "terminal.ansiGreen": "#ef8a39",
        "terminal.ansiYellow": "#e5c07b",
        "terminal.ansiBlue": "#61afef",
        "terminal.ansiMagenta": "#c586c0",
        "terminal.ansiCyan": "#56b6c2",
        "terminal.ansiWhite": "#dcdcdc",
        "terminal.ansiBrightBlack": "#7f848e",
        "terminal.ansiBrightRed": "#ff5f56",
        "terminal.ansiBrightGreen": "#ef8a39",
        "terminal.ansiBrightYellow": "#f0e68c",
        "terminal.ansiBrightBlue": "#61afef",
        "terminal.ansiBrightMagenta": "#d19a66",
        "terminal.ansiBrightCyan": "#56b6c2",
        "terminal.ansiBrightWhite": "#fff0f595",
        "terminalCursor.background": "#ef8a39",
        "terminalCursor.foreground": "#ef8a39",
        "terminal.background": "#222229",
        "panel.background": "#222229",
        "panel.border": "#1D1E24",
        "panelTitle.activeForeground": "#fff0f595",
        "panelTitle.inactiveForeground": "#6e7681"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator",
                "keyword.other",
                "keyword.new",
                "keyword.async"
            ],
            "settings": {
                "foreground": "#7168eb",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.language",
                "variable.other.local",
                "variable.other.parameter"
            ],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": [
                "constant",
                "constant.language",
                "constant.numeric",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#ef8a39"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#6c86c0"
            }
        },
        {
            "scope": ["comment", "comment.block", "comment.line"],
            "settings": {
                "foreground": "#707070",
                "fontStyle": "italic"
            }
        },

        {
            "scope": [
                "entity.name.function",
                "meta.function",
                "variable.function"
            ],
            "settings": {
                "foreground": "#eed258"
            }
        },

        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#a386f1",
                "fontStyle": ""
            }
        },
        {
            "scope": ["entity.name.class", "entity.name.type"],
            "settings": {
                "foreground": "#d89832",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "punctuation",
                "punctuation.separator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#d89832"
            }
        },
        {
            "scope": ["variable.other.property", "variable.other.object"],
            "settings": {
                "foreground": "#95b1fd"
            }
        },
        {
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF6F6F"
            }
        },
        {
            "scope": [
                "meta.tag",
                "meta.link",
                "meta.selector",
                "meta.property-name"
            ],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": ["variable.other.color", "variable.other.constant"],
            "settings": {
                "foreground": "#A0A0A0",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.comparison"
            ],
            "settings": {
                "foreground": "#ef8a39"
            }
        },
        {
            "scope": [
                "variable.other.import",
                "variable.other.js",
                "variable.other.scss",
                "variable.other.ts"
            ],
            "settings": {
                "foreground": "#FFAB6F"
            }
        },
        {
            "scope": ["variable.language.boolean", "variable.other.state"],
            "settings": {
                "foreground": "#F5A700"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "entity.name.tag.js",
                "entity.name.tag.jsx"
            ],
            "settings": {
                "foreground": "#d89832",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.tag.section",
                "entity.name.tag.div",
                "entity.name.tag.header",
                "entity.name.tag.footer",
                "entity.name.tag.article",
                "entity.name.tag.aside"
            ],
            "settings": {
                "foreground": "#7BB9C5"
            }
        },
        {
            "scope": [
                "meta.jsx.children",
                "meta.jsx.attribute",
                "meta.jsx.self-closing",
                "meta.jsx.end-tag"
            ],
            "settings": {
                "foreground": "#fff0f595"
            }
        },
        {
            "scope": ["entity.name.component", "entity.name.jsx"],
            "settings": {
                "foreground": "#a37c9b"
            }
        },
        {
            "scope": ["variable.other.jsx", "variable.other.js"],
            "settings": {
                "foreground": "#F5A700"
            }
        },
        {
            "scope": ["entity.other.attribute-name.css"],
            "settings": {
                "foreground": "#66c2c2",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["support.type.property-name.css"],
            "settings": {
                "foreground": "#8b96c5"
            }
        },
        {
            "scope": ["entity.name.tag.css"],
            "settings": {
                "foreground": "#d89832"
            }
        },
        {
            "scope": [
                "support.constant.property-value.css",
                "support.constant.property-value.scss"
            ],
            "settings": {
                "foreground": "#d89832"
            }
        },
        {
            "scope": ["support.variable.color.css"],
            "settings": {
                "foreground": "#ff7373"
            }
        },
        {
            "scope": ["keyword.control.import.ts", "keyword.control.export.ts"],
            "settings": {
                "foreground": "#776ef5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["support.type.primitive.ts"],
            "settings": {
                "foreground": "#747c80"
            }
        },
        {
            "scope": ["entity.name.type.ts"],
            "settings": {
                "foreground": "#a37c9b",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name",
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#888888",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.selector",
                "meta.selector.class",
                "meta.selector.id"
            ],
            "settings": {
                "foreground": "#FF6347",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["entity.other.attribute-name.class.tailwind"],
            "settings": {
                "foreground": "#9F7AEA"
            }
        },
        {
            "scope": ["variable.language.next"],
            "settings": {
                "foreground": "#ff7e5f"
            }
        }
    ]
}
